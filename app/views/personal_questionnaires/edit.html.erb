<h1>Edit Questionnaire</h1>
<%= form_with(model: @questionnaire, url: api_personal_questionnaire_path, local: true) do |form| %>
  Medical:<br>
  <%= form.text_field :medical %> <br>
  Emergency Contact: <br>
  <%= form.text_field :emergency_contact_name %> <br>  
  <br>
  <%= form.submit "Submit" %>
<% end %>
<%= link_to 'Back', personal_questionnaires_path %>