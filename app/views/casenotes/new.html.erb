<h1>New Casenotes</h1>
<%= form_with scope: :casenote, url: api_casenotes_path, local: true do |form| %>
  Title:<br>
  <%= form.text_field :title %> <br>
  Description: <br>
  <%= form.text_field :description %> <br>
  Participant: <br>
	<%= form.collection_select :participant_id, @participants, :id, :name, prompt: true %>
	<br>
  Internal: <br>
  <%= form.check_box :internal %> <Br>
  <%= form.submit "Submit" %>
<% end %>