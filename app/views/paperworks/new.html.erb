<h1>New Paperwork</h1>
<%= form_with scope: :paperwork, url: api_paperworks_path, local: true do |form| %>
	Title:<br>
	<%= form.text_field :title %> <br>
	Link: <br>
	<%= form.text_field :link %>
	<br>
	Participant: <br>
	<%= form.collection_select :participant_id, @participants, :id, :name, prompt: true %>
	<br>
  Agree: <br>
  <%= form.check_box :agree %> <Br>
	<%= form.submit "Submit" %>
<% end %>
<%= link_to 'Back', paperworks_path %>