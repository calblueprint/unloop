{{/if}} --}}

{{#if wantStyles}}
import styles from './styles';
{{/if}}
{{!-- {{#if wantMessages}}
import messages from './messages';
{{/if}} --}}

function {{ properCase name }}({{#if wantStyles}}{ classes }{{/if}}) {
return (
<div{{#if wantStyles}} className={ classes.root }{{/if}}>
  {{!-- {{#if wantMessages}}
      <FormattedMessage {...messages.header} />
    {{/if}} --}}
  </div>
  );
  }

