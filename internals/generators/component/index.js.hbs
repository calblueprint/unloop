  {{#if wantStyles}}
  classes: PropTypes.object.isRequired,
  {{/if}}
  };

  export default compose(
  {{#if wantStyles}}
  withStyles(styles),
  {{/if}}
  {{#if memo}}
  memo,
  {{/if}}
  )({{ properCase name }});
